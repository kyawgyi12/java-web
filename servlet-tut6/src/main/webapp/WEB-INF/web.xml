<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>servlet-tut6</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>RequestTestServlet</display-name>
    <servlet-name>RequestTestServlet</servlet-name>
    <servlet-class>com.jdc.tut6.RequestTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestTestServlet</servlet-name>
    <url-pattern>/RequestTestServlet</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.jdc.tut6.RequestListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.jdc.tut6.SessionListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.jdc.tut6.ContextListener</listener-class>
  </listener>
</web-app>